@{
    ViewData["Title"] = "Home Page";
}
 <div class="container">

        <div class="row align-items-start">


          <div class="col">

            <h3>formulario de registro</h3>
            
            <div class="mb-3">
              <label for="exampleFormControlInput1" class="form-label">Email address</label>
              <input type="email" class="form-control" id="exampleFormControlInput1" placeholder="name@example.com">
            </div>
            
            <div class="mb-3"><select class="form-select" aria-label="Default select example">
              <option selected>sexo</option>
              <option value="1">femenino</option>
              <option value="2">mascuino</option>
              <option value="3">prefiero no decirlo</option>
            </select></div>
            
            
            
            <div class="mb-3">
              <label for="exampleFormControlTextarea1" class="form-label">Example textarea</label>
              <textarea class="form-control" id="exampleFormControlTextarea1" rows="3"></textarea>
            </div>
            
            <div class="mb-3">
              <button type="button" class="btn btn-dark ">Enviar</button>
              <button type="button" class="btn btn-secondary ">Cancelar</button>
              
            </div>
          </div>
          <div class="col">
            <h3>casos de exito</h3>
            <div id="carouselExample" class="carousel slide">
              <div class="carousel-inner">
                <div class="carousel-item active">
                  <img src="./imagenes/images.jpeg" class="d-block w-100" alt="...">
                </div>
                <div class="carousel-item">
                  <img src="./imagenes/images 2.jpeg" class="d-block w-100" alt="...">
                </div>
               
              </div>
              <button class="carousel-control-prev" type="button" data-bs-target="#carouselExample" data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
              </button>
              <button class="carousel-control-next" type="button" data-bs-target="#carouselExample" data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
              </button>
            </div>



          </div>
            
        </div>
          
        </div>